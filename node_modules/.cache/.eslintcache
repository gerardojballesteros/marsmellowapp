  <div className=\"footer__content\">\n               <p>Copyright Â© 2022 | Marsmellow App, powered by <a href=\"https://www.gerardoballesteros.com\" target=\"_blank\">Gerardo Ballesteros</a></p>\n               <div>\n                <p>More information </p>\n                <a href=\"https://api.nasa.gov\" target=\"_blank\"><Icon id=\"nasa\" icon=\"simple-icons:nasa\" className=\"nasa-icon\" color=\"white\" width=\"50px\" /></a>\n               </div>\n           </div>\n       </footer>\n    )\n}\n\nexport default Footer\n","/Users/gerardoballesteros/marsmellowapp/src/components/rovers/RoversScreen.js",[],[],"/Users/gerardoballesteros/marsmellowapp/src/components/home/Home.js",[],[],"/Users/gerardoballesteros/marsmellowapp/src/components/home/components/Weather.js",[],[],"/Users/gerardoballesteros/marsmellowapp/src/components/home/components/Banner.js",[],[],"/Users/gerardoballesteros/marsmellowapp/src/components/home/components/ImageMars.js",["62","63","64"],[],"import React, {useEffect, useState} from 'react'\nimport Masonry, {ResponsiveMasonry} from \"react-responsive-masonry\";\nimport LazyLoad from 'react-lazyload';\nimport { Icon } from '@iconify/react';\nimport 'animate.css';\n\nconst ImageMars = () => {\n    const [images, setImages] = useState([]);\n    const [show, setShow] = useState(false);\n    const [Photos2, setPhotos] = useState();\n    const [isOpened, setIsOpened] = useState(false);\n    const [rovers, setRovers] = useState('perseverance');\n    const [sol, setSol] = useState('433');\n    useEffect(() => {\n        api();\n    }, [rovers, sol])\n    const api = async()=> {                                                                                                                                                                                                              \n        const url = `https://mars-photos.herokuapp.com//api/v1/rovers/${rovers}/photos?sol=${sol}`;\n        const res = await fetch(url);\n        const data = await res.json();\n        const dataShowImage = data.photos.slice(0, 100).map( response => {\n            return {\n                id: response.id,\n                Photos: response.img_src\n            }\n        });\n        setImages(dataShowImage)\n    }\n    const handleShow = (Photos) => {\n        setPhotos(Photos)\n        setShow(true);\n        setIsOpened(true);\n    }\n    const handleClose = () => {\n        setShow(false);\n        setPhotos(null);\n        setIsOpened(false);\n    }\n    const handleChange = (e) => {\n        setSol(e.target.value);\n      }\n    return (\n        <> \n            <section className=\"images-mars\">\n                <div className=\"images-mars__content\">\n                    <article className=\"txt\">\n                        <h4>Mars as <strong>never seen before.</strong></h4>\n                        <p>Through the Marsmellow app you will be able to see \n                            the latest images photographed on mars by the spirit, opportunity, curiosity and perseverance rovers. \n                            Remember that these images are updated by the image bank of the <b>National Aeronautics and Space Administration (Nasa)</b></p>\n                    </article>\n                    <div className=\"btn-filter\">\n                        <button className=\"btn-white\" type=\"button\" onClick={()=> setRovers('spirit')}>Spirit</button>\n                        <button className=\"btn-white\" type=\"button\" onClick={()=> setRovers('curiosity')}>Curiosity</button>\n                        <button className=\"btn-white\" type=\"button\" onClick={()=> setRovers('opportunity')}>Opportunity</button>\n                        <button className=\"btn-white\" type=\"button\" onClick={()=> setRovers('perseverance')}>Perseverance</button>\n                        <label className=\"input-mars\" htmlFor=\"input-mars\">\n                            <p>Day or Sol</p>\n                            <input type=\"number\" id=\"input-mars\" onChange={handleChange} value={sol} />\n                        </label>\n                    </div>  \n                    <article className=\"title-rover\">\n                        <p>Images of the <strong>{rovers} Rover</strong></p>\n                    </article>\n                    <ResponsiveMasonry className=\"gallery\"  columnsCountBreakPoints={{350: 2, 750: 2, 900: 3, 1200: 5, 1600: 5}}>\n                        <Masonry gutter=\"10px\">\n                        {\n                            images.map( ({ id, Photos }) => {\n                                return (\n                                    <div key={id}>\n                                        <LazyLoad height={300}  >\n                                            <img \n                                            src={Photos} \n                                            onClick={() => handleShow(Photos)}\n                                            style={{width: \"100%\", display: \"block\", cursor: \"pointer\"}}\n                                            alt=\"\" className=\"animate__animated animate__fadeIn \" />\n                                        </LazyLoad>\n                                    </div>\n                                )\n                            })\n                        }\n                        </Masonry> \n                    </ResponsiveMasonry> \n                </div>\n            </section>\n            { isOpened && (\n                <div className=\"modal\">\n                    <div className=\"modal__content\">\n                        <div className=\"btn-close\">\n                            <a onClick={handleClose}><Icon icon=\"ep:close\" color=\"white\" /></a>\n                        </div>\n                        <img src={Photos2} alt=\"\" />\n                    </div>\n                </div>\n            )\n            }\n        </>\n    )\n}\n\nexport default ImageMars\n",{"ruleId":"65","severity":1,"message":"66","line":66